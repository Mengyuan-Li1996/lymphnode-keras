{
"feature": "germinal",
"tasktype": "binary",
"magnification": "10x", 
"loss": ["weightedBinaryCrossEntropy"], 
"modelname": "$model_germ_10x_adam_$loss_$augment", 
"ratio": 0.9,
"batchSize": 4,
"imageDims": 1024,
"epoch": 100, 
"augmentation": 
	{
	"methods": [["Flip","Rotate90","Color"]],
	"parameters":
		{
		"saturation":[0.5,1.2],
		"brightness":0.25,
		"hue":0.05,
		"contrast":[0.8,1.5],
		"flipProb":0.5,
		"rotateProb":0.5,
		"colorProb":0.5
		}
	},	
"nClasses": 1, 
"weights": [8.8], 
"dropout":[0],
"model": 
	{
	"network":"multiscale",
        "parameters":
		{
		"filters":[32,64,128,256,512],
		"finalActivation":"sigmoid"
		}
	},	
"optimizer": 
	{ 
	"method": "adam", 
	"parameters": 
		{
		"learning_rate": 0.001, 
		"amsgrad": true
		},
        "decaymethod":{"custom":{
		                "method":null,
				"decayparams":   
		                             {
		                             "maxEpochs": 50, 
		                             "power": 3, 
		                             "initLr": 0.001
					     }
				},
	              "keras":"piecewise",
		      "decayparams": {
			             "boundaries":[30, 60],
				     "values": [0.001,0.0005,0.0001]
			 	     }
		      		}
			}, 
"metric": "diceCoef", 
"normalize":"True", 
"padding":"same", 
"multi":
	{
	"flag":true,
	"num": 4
	},  
"modelapi":"subclass", 
"api":"custom",
"activationthreshold":0.5,
"stopthresholds": 
	{
	"first":
		{
		"metric":0.8,
		"epochs":50
		},
		"second":
		{
		"metric":0.75,
		"epochs":75
		}
	},
"step":1024,
"upTypeName":"upsampling"}
